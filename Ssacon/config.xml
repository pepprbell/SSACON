<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SSACON</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        SSAFY with Samsung SDI
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon background="res/logo.png" density="ldpi" foreground="res/logo.png" />
        <icon background="res/logo.png" density="mdpi" foreground="res/logo.png" />
        <icon background="res/logo.png" density="hdpi" foreground="res/logo.png" />
        <icon background="res/logo.png" density="xhdpi" foreground="res/logo.png" />
        <icon background="res/logo.png" density="xxhdpi" foreground="res/logo.png" />
        <icon background="res/logo.png" density="xxxhdpi" foreground="res/logo.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="AndroidXEnabled" value="true" />
</widget>
